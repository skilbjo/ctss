                  VECTOR VALUES MZE = 4K11                                                        *014 LM00016    
                  VECTOR VALUES TFUDG = 1800                                                      *015 LM00017    
                  VECTOR VALUES TABLIM = 200                                                      *016 LM00018    
                  VECTOR VALUES LODLIM = 2000                                                     *017 LM00019    
                  VECTOR VALUES LPREAM = 12                                                       *018 LM00020    
