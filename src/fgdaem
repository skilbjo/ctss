                                                                                                       LM00031    
                  BOOLEAN SW                                                                      *028 LM00006    
                                                                                                       LM00007    
                  VECTOR VALUES URGENT = $URGENT$                                                 *029 LM00008    
                  VECTOR VALUES MAIL = $  MAIL$                                                   *030 LM00009    
