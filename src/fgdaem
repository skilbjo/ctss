      00001  000000000000     
      00002  652551312670     
                  
      00003 -0500 00 0 00037  UMOUNT CAL     =HUMOUNT      GET SUPERVISOR NAME
      00004  0020 00 0 00010         TRA     MOUNT+1       SKIP INTO ROUTINE  
                  
      00005 -0500 00 0 00040  VERIFY CAL     =HVERIFY      GET SUPERVISOR NAME
      00006  0020 00 0 00010         TRA     *+2           SKIP MOUNT ENTRY   
                  
      00007 -0500 00 0 00036  MOUNT  CAL     =HMOUNT       GET SUPERVISOR NAME
      00010  0602 00 0 00035         SLW     SUPCAL        SAVE BCD NAME
      00011  0636 00 4 00013         SCA     IR4,4         SAVE RETURN  
      00012 -0774 00 4 00004         AXC     4,4           GET PARAMETERS     
      00013  0500 00 4 00000  IR4    CLA     **,4          ..     
      00014  0601 00 4 00021         STO     MT,4          SAVE IN THIS ROUTINE     
      00015  1 00001 4 00016         TXI     *+1,4,1       NEXT PARAMETER     
      00016  3 77774 4 00013         TXH     *-3,4,-4      TEST FOR COMPLETION
                  
      00017 -0774 00 4 00021         AXC     MT,4          SET UP 0,4 RETURN  
      00020  0101 00 0 00035         TIA     SUPCAL        ENTER SUPERVISOR   
      00021  0020 00 0 00030  MT     TRA     SLTIME        SLEEP A WHILE IF 0,4 RETURN    
      00022  0 00000 0 00000         ***     ** 
      00023  0 00000 0 00000         ***     ** 
