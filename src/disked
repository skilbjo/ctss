1     MAD PROGRAM LISTING ... ... ...                 FILE  SCNMF1   MAD            05/15/71    1241.0
-                 
                  PROGRAM TO SCAN THE MFD AND RETURN USERS IN ORDER                                    LM00001    
                    CROCKED FOR MININUM LENGTH SO THE STAND-ALONE                                      LM00002    
                      EDITORS CAN BE LOADED VIA FMS.  04/18/68                                         LM00003    
                                                                                                       LM00004    
                  EXTERNAL FUNCTION (PB,PG,END)                                                   *001 LM00005    
                  ENTRY TO SCNMFD.                                                                *002 LM00006    
                                                                                                       LM00007    
                  INSERT FILE COMDSK                                                              *003 LM00008    
                 ...COMMON PACKAGE FOR DISK EDITOR -DSKEDT                                        *003 LM00001    
                  PROGRAM COMMON B4SWT,PRIVIL,CARSWT,PRNSWT                                       *004 LM00002    
                  PROGRAM COMMON PROB,PROG,FNAM1,FNAM2,MODE,C,ERCOD,DUM                           *005 LM00003    
                  PROGRAM COMMON PROB1, PROG1, DATE,TIME,BUF                                      *006 LM00004    
                  FUNCTION NAME CTLF,EXIT                                                         *007 LM00005    
                  DIMENSION C(14),BUF(864)                                                        *008 LM00006    
                  VECTOR VALUES ONOFRT = 7                                                        *009 LM00007    
                  VECTOR VALUES ONRT = 5                                                          *010 LM00008    
                  VECTOR VALUES OFRT = 3                                                          *011 LM00009    
                  VECTOR VALUES ONOF = 6                                                          *012 LM00010    
                  VECTOR VALUES ON = 4                                                            *013 LM00011    
                  VECTOR VALUES OF = 2                                                            *014 LM00012    
                  VECTOR VALUES EOFCT = 0                                                         *015 LM00013    
                  VECTOR VALUES MZE = -0                                                          *016 LM00014    
                  VECTOR VALUES DSKEDT = $DSKEDT$                                                 *017 LM00015    
                  VECTOR VALUES FILE = $.FILE.$                                                   *018 LM00016    
                  VECTOR VALUES CRYMAP = $CRYMAP$                                                 *019 LM00017    
                  NORMAL MODE IS INTEGER                                                          *020 LM00018    
                                                                                                       LM00009    
                  WHENEVER NEWSW                                                                  *021 LM00010    
                    ATTACH.(MFD,FILE,BAD)                                                         *022 LM00011    
                    CLOSE.(MFD,FILE,OPNMFD)                                                       *023 LM00012    
      OPNMFD        OPEN.($R$,MFD,FILE,-0,-0,BAD)                                                 *024 LM00013    
                    NEWSW=0B                                                                      *025 LM00014    
                    RDFILE.(MFD,FILE,0,MFDENT(6)...3,EOF,-0,NOREAD)                               *026 LM00015    
                  END OF CONDITIONAL                                                              *027 LM00016    
                                                                                                       LM00017    
      RDMFD       RDWAIT.(MFD,FILE,0,MFDENT(6)...7,EOF,-0,NOREAD)                                 *028 LM00018    
                  PB=MFDENT(6)                                                                    *029 LM00019    
                  PG=MFDENT(5)                                                                    *030 LM00020    
                  WHENEVER PB.V.PG.E.0.OR.PG.E.FILE, TRANSFER TO RDMFD                            *031 LM00021    
                  FUNCTION RETURN 1                                                               *032 LM00022    
                                                                                                       LM00023    
      EOF         NEWSW=1B                                                                        *033 LM00024    
                  CLOSE.(MFD,FILE,NOCLOS)                                                         *034 LM00025    
                  TRANSFER TO END                                                                 *035 LM00026    
                                                                                                       LM00027    
      BAD         ERR.(MSG1...MSG1(2),ONOFRT,BADXIT)                                              *036 LM00028    
                    VECTOR VALUES MSG1=$CAN'T PROCESS MFD$                                        *037 LM00029    
      BADXIT      FUNCTION RETURN 0                                                               *038 LM00030    
                                                                                                       LM00031    
      NOREAD      ERR.(MSG2...MSG2(2),ONOFRT,BADXIT)                                              *039 LM00032    
                    VECTOR VALUES MSG2=$ERROR READING MFD$                                        *040 LM00033    
                                                                                                       LM00034    
      NOCLOS      ERR.(MSG3...MSG3(2),ONOFRT,BADXIT)                                              *041 LM00035    
