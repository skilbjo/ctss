SCNMFD   BCD 05/15/71 1243.0    6355         00000    
1     MAD PROGRAM LISTING ... ... ...                 FILE  SCNMFD   MAD            05/15/71    1241.3
-                 
                 ...PROGRAM TO SCAN THE MFD FILE AND RETURN USERS IN ORDER.                            LM00001    
                                                                                                       LM00002    
                  EXTERNAL FUNCTION (PB, PG, END)                                                 *001 LM00003    
                  ENTRY TO SCNMFD.                                                                *002 LM00004    
                                                                                                       LM00005    
                  INSERT FILE COMDSK                                                              *003 LM00006    
                 ...COMMON PACKAGE FOR DISK EDITOR -DSKEDT                                        *003 LM00001    
                  PROGRAM COMMON B4SWT,PRIVIL,CARSWT,PRNSWT                                       *004 LM00002    
                  PROGRAM COMMON PROB,PROG,FNAM1,FNAM2,MODE,C,ERCOD,DUM                           *005 LM00003    
                  PROGRAM COMMON PROB1, PROG1, DATE,TIME,BUF                                      *006 LM00004    
                  FUNCTION NAME CTLF,EXIT                                                         *007 LM00005    
                  DIMENSION C(14),BUF(864)                                                        *008 LM00006    
                  VECTOR VALUES ONOFRT = 7                                                        *009 LM00007    
                  VECTOR VALUES ONRT = 5                                                          *010 LM00008    
                  VECTOR VALUES OFRT = 3                                                          *011 LM00009    
                  VECTOR VALUES ONOF = 6                                                          *012 LM00010    
                  VECTOR VALUES ON = 4                                                            *013 LM00011    
                  VECTOR VALUES OF = 2                                                            *014 LM00012    
                  VECTOR VALUES EOFCT = 0                                                         *015 LM00013    
                  VECTOR VALUES MZE = -0                                                          *016 LM00014    
                  VECTOR VALUES DSKEDT = $DSKEDT$                                                 *017 LM00015    
                  VECTOR VALUES FILE = $.FILE.$                                                   *018 LM00016    
                  VECTOR VALUES CRYMAP = $CRYMAP$                                                 *019 LM00017    
                  NORMAL MODE IS INTEGER                                                          *020 LM00018    
                  WHENEVER  NEWSW                                                                 *021 LM00007    
                       ATTACH.(MFD,FILE,ER10)                                                     *022 LM00008    
                        CLOSE.(MFD,FILE,OPNMFD)                                                   *023 LM00009    
      OPNMFD           OPEN.( $R$, MFD, FILE, MZE,MZE,ER10,ERCOD)                                 *024 LM00010    
                       NEWSW = 0B                                                                 *025 LM00011    
                     EOFSWT = 0                                                                   *026 LM00012    
                        EOFCT = 432                                                               *027 LM00013    
                       M = 2                                                                      *028 LM00014    
                     J = 432                                                                      *029 LM00015    
                     K = 864                                                                      *030 LM00016    
                     FLAG = 10                                                                    *031 LM00017    
                     RDFILE.(MFD,FILE,0,BUFFR(J)...432,EOF1,EOFCT,ER11,ERCOD)                     *032 LM00018    
                     FLAG = 15                                                                    *033 LM00019    
                     RDFILE.(MFD,FILE,0,BUFFR(K)...432,EOF1,EOFCT,ER11,ERCOD)                     *034 LM00020    
                     FLAG = 20                                                                    *035 LM00021    
                  END OF CONDITIONAL                                                              *036 LM00022    
                                                                                                       LM00023    
      READMD      READBF.                                                                         *037 LM00024    
                  PB = BUFFR(J-M)                                                                 *038 LM00025    
                  READBF.                                                                         *039 LM00026    
                  PG = BUFFR(J-M)                                                                 *040 LM00027    
                  THROUGH FLIP, FOR I = 1,1, I .G. 5                                              *041 LM00028    
      FLIP        READBF.                                                                         *042 LM00029    
                  WHENEVER PG .E. 0 .AND. PB .E. 0, TRANSFER TO READMD                            *043 LM00030    
                  WHENEVER PG .E. $(FILE)$, TRANSFER TO READMD                                    *044 LM00031    
                  FUNCTION RETURN 1                                                               *045 LM00032    
                                                                                                       LM00033    
      EOF1        CLOSE.(MFD,FILE,ER12,ERCOD)                                                     *046 LM00034    
                  EOFSWT = 77777K                                                                 *047 LM00035    
                  WHENEVER FLAG .E. 10,TRANSFER TO READMD                                         *048 LM00036    
                  EOFTMP = EOFCT                                                                  *049 LM00037    
                  EOFCT = 432                                                                     *050 LM00038    
                  WHENEVER  FLAG .E. 15,TRANSFER TO READMD                                        *051 LM00039    
                  TRANSFER TO RDXT                                                                *052 LM00040    
      EOF3        WHENEVER FLAG .E. 10,TRANSFER TO EOF                                            *053 LM00041    
                  EOFCT = EOFTMP                                                                  *054 LM00042    
                  FLAG = 10                                                                       *055 LM00043    
                  TRANSFER TO RDXT                                                                *056 LM00044    
      EOF         NEWSW = 1B                                                                      *057 LM00045    
                  TRANSFER TO END                                                                 *058 LM00046    
                                                                                                       LM00047    
                 ... ERROR RETURN                                                                      LM00048    
                                                                                                       LM00049    
      ER10        ERR.(MSG10...MSG10(2),ONOFRT,NXTRT)                                             *059 LM00050    
                       VECTOR VALUES MSG10 = $CAN'T PROCESS MFD$                                  *060 LM00051    
      NXTRT       FUNCTION RETURN 0                                                               *061 LM00052    
      ER11        ERR.(MSG11...MSG11(2),ONOFRT,NXTRT)                                             *062 LM00053    
                       VECTOR VALUES MSG11 = $ERROR READING MFD$                                  *063 LM00054    
      ER12        ERR.(MSG12...MSG12(2),ONOFRT,NXTRT)                                             *064 LM00055    
                     VECTOR VALUES MSG12 = $CAN'T CLOSE M.F.D.$                                   *065 LM00056    
