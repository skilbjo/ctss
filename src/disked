                                                                                                       LM00044    
                  INSERT FILE COMDSK                                                              *042 LM00006    
                 ...COMMON PACKAGE FOR DISK EDITOR -DSKEDT                                        *042 LM00001    
                  PROGRAM COMMON B4SWT,PRIVIL,CARSWT,PRNSWT                                       *043 LM00002    
                  PROGRAM COMMON PROB,PROG,FNAM1,FNAM2,MODE,C,ERCOD,DUM                           *044 LM00003    
                  PROGRAM COMMON PROB1, PROG1, DATE,TIME,BUF                                      *045 LM00004    
                  FUNCTION NAME CTLF,EXIT                                                         *046 LM00005    
                  DIMENSION C(14),BUF(864)                                                        *047 LM00006    
                  VECTOR VALUES ONOFRT = 7                                                        *048 LM00007    
                  VECTOR VALUES ONRT = 5                                                          *049 LM00008    
                  VECTOR VALUES OFRT = 3                                                          *050 LM00009    
                  VECTOR VALUES ONOF = 6                                                          *051 LM00010    
                  VECTOR VALUES ON = 4                                                            *052 LM00011    
                  VECTOR VALUES OF = 2                                                            *053 LM00012    
                  VECTOR VALUES EOFCT = 0                                                         *054 LM00013    
                  VECTOR VALUES MZE = -0                                                          *055 LM00014    
                  VECTOR VALUES DSKEDT = $DSKEDT$                                                 *056 LM00015    
                  VECTOR VALUES FILE = $.FILE.$                                                   *057 LM00016    
                  VECTOR VALUES CRYMAP = $CRYMAP$                                                 *058 LM00017    
                  NORMAL MODE IS INTEGER                                                          *059 LM00018    
                  EQUIVALENCE(RDCD,CRDEOF)                                                        *060 LM00007    
                  PRIVIL = PRIV.(0)                                                               *061 LM00008    
                  BPNCHI.(2202K)                                                                  *062 LM00009    
                  BINREW.                                                                         *063 LM00010    
                  GETTIM.(DATE,TIME)                                                              *064 LM00011    
                  IDD(2) = DATE                                                                   *065 LM00012    
                  IDD(4) = TIME                                                                   *066 LM00013    
                  BPUNCH.(IDD(0)...IDD(4))                                                        *067 LM00014    
                     VECTOR VALUES IDD = $ ID CARDS   $,$$,$$,$$                                  *068 LM00015    
                                                                                                       LM00016    
                  UPDMFD.(DSKEDT,FILE,ALRDY1)                                                     *069 LM00017    
                  ATTACH.(DSKEDT,FILE)                                                            *070 LM00018    
                  ALLOT.(2,100)                                                                   *071 LM00019    
      ALRDY1      CARSWT = 0                                                                      *072 LM00020    
                  ATTACH.(DSKEDT,FILE)                                                            *073 LM00021    
                  RESETF.(AX1)                                                                    *074 LM00022    
      AX1         CLOSE.(STAT,FILE,OPSTAT)                                                        *075 LM00023    
                                                                                                       LM00024    
      OPSTAT      OPEN.($RW$,STAT,FILE,MZE,MZE,ER13,ERCOD)                                        *076 LM00025    
                  BUFFER.(STAT,FILE,BUFT(432)...432,ER13,ERCOD)                                   *077 LM00026    
                                                                                                       LM00027    
                  WHENEVER .SW. 2, TRANSFER TO QUIT                                               *078 LM00028    
                  WHENEVER .SW. 4, TRANSFER TO GOCARD                                             *079 LM00029    
      RQST        ONLINE.(MESS...MESS(1))                                                         *080 LM00030    
                     VECTOR VALUES MESS = $RQUEST FILES$                                          *081 LM00031    
                  STPCHG.                                                                         *082 LM00032    
                  UPDCHG.                                                                         *083 LM00033    
                  WHENEVER .SW. 3                                                                 *084 LM00034    
                       PROB = DAEPRB                                                              *085 LM00035    
                       PROG = DAEPRG                                                              *086 LM00036    
                       ATTACH.(PROB,PROG,ER14)                                                    *087 LM00037    
                       FSTATE.(OUT,RQU,A(8)...8,ER14)                                             *088 LM00038    
                       ONLINE.(DEMON...DEMON(3))                                                  *089 LM00039    
                          VECTOR VALUES DEMON = $STARTING DAEMON REQUESTS$                        *090 LM00040    
                       TRANSFER TO ATTDAE                                                         *091 LM00041    
                  END OF CONDITIONAL                                                              *092 LM00042    
                                                                                                       LM00043    
      SCMFD       WHENEVER .SW. 3                                                                 *093 LM00044    
                     ONLINE.(SWT3T1...SWT3T1(7))                                                  *094 LM00045    
                     ONLINE.(SWT3T2...SWT3T2(3))                                                  *095 LM00046    
                     ONLINE.(SWT3T3...SWT3T3(7))                                                  *096 LM00047    
                  VECTOR VALUES SWT3T1=$FINISHED DAEMON. RESET SWT 3 TO CONTINUE RQUEST$          *097 LM00048    
                     VECTOR VALUES SWT3T2 = $SWT 3 OR 2 DWN TO QUIT.$                             *098 LM00049    
                     VECTOR VALUES SWT3T3 =$SWT 3 AND 2 UP WITH 4 DWN TO DO INPUT TAPE.$          *099 LM00050    
                     PAUSE NO. 7                                                                  *100 LM00051    
                     WHENEVER .SW. 3, TRANSFER TO QUIT                                            *101 LM00052    
                  END OF CONDITIONAL                                                              *102 LM00053    
                  WHENEVER .SW. 2,TRANSFER TO QUIT                                                *103 LM00054    
                  WHENEVER .SW. 4, TRANSFER TO GOCARD                                             *104 LM00055    
                                                                                                       LM00056    
                  TILOCK.(SCMFD)                                                                  *105 LM00057    
                  SUCESS = SCNMFD.(PROB,PROG,ENDMFD)                                              *106 LM00058    
                  WHENEVER SUCESS .E. 0, TRANSFER TO ENDMFD                                       *107 LM00059    
                  ATTACH.(PROB,PROG,SCMFD)                                                        *108 LM00060    
                                                                                                       LM00061    
                 ...SWT 6 -UP -DONT PROCESS TEMP MODE RQUEST FILES                                     LM00062    
                          DWN - PROCESS ALL RQUEST FILES FOR RERUN                                     LM00063    
                  FSTATE.(OUT,RQU,A(8)...8,SCMFD)                                                 *109 LM00064    
      ATTDAE         WHENEVER .NOT. .SW. 6                                                        *110 LM00065    
                        WHENEVER A(7) .A. 1 .E. 1                                                 *111 LM00066    
                           DELFIL.(OUT,RQU,SCMFD)                                                 *112 LM00067    
                           TRANSFER TO SCMFD                                                      *113 LM00068    
                                                                                                       LM00069    
                        END OF CONDITIONAL                                                        *114 LM00070    
                                                                                                       LM00071    
                     END OF CONDITIONAL                                                           *115 LM00072    
                                                                                                       LM00073    
                  CHARGE.(PROB,PROG)                                                              *116 LM00074    
                  GETTIM.(DATE,TIME)                                                              *117 LM00075    
                  CHFILE.(OUT,RQU,124K,MZE,MZE,ER3,ERCOD)                                         *118 LM00076    
                  OPEN.($R$,OUT,RQU,MZE,MZE,SCMFD)                                                *119 LM00077    
                  BUFFER.(OUT,RQU,BUFFR(470)...470,ER1,ERCOD)                                     *120 LM00078    
                  TILOCK.(NAMES)                                                                  *121 LM00079    
                                                                                                       LM00080    
      READ        STPCH1.                                                                         *122 LM00081    
                  RDWAIT.(OUT,RQU,0,C(14)...14,EOF,EOFCT,ER3,ERCOD)                               *123 LM00082    
                  ERR.(C(14)...C(1), ONOFRT,CONTRL)                                               *124 LM00083    
      CONTRL      FF = SCANI.(C(14),ER4)                                                          *125 LM00084    
                  WHENEVER FF .E. $     C$, TRANSFER TO NAMES                                     *126 LM00085    
      FINDI       THROUGH FINDI, FOR I =1,1,I .G. CTL .OR. CTL(I) .E. FF                          *127 LM00086    
                  WHENEVER I .G. CTL, TRANSFER TO ER6                                             *128 LM00087    
                  WHENEVER FF .E. $UPDMFD$ .OR. FF .E. $  LINK$, TRANSFER TO ER5                  *129 LM00088    
                  CHARG1.(CTLCHG(I))                                                              *130 LM00089    
                  PROB1 = C(2)                                                                    *131 LM00090    
                  PROG1 = C(1)                                                                    *132 LM00091    
                  WHENEVER FF .E. $ PRINT$ .OR. FF .E. $PRNDEL$,                                  *133 LM00092    
                1    NPRINT.(EJECT...EJECT)                                                       *133 LM00093    
      NAMES       FNAM1 = SCAN.(READ)                                                             *134 LM00094    
                  FNAM2 = SCAN.(ER4)                                                              *135 LM00095    
                  WHENEVER FNAM1 .E. $OUTPUT$ .AND. FNAM2 .E. $RQUEST$, TRANSFER TO NAMES         *136 LM00096    
                                                                                                       LM00097    
                 ...GO TO DESIRED CONTROL FUNCTION                                                     LM00098    
                  SUCESS= CTLF(I).                                                                *137 LM00099    
                  UPDATE.                                                                         *138 LM00100    
                  TRANSFER TO NAMES                                                               *139 LM00101    
                                                                                                       LM00102    
      EOF         CLOSE.(OUT,RQU,ER12,ERCOD)                                                      *140 LM00103    
                  CHFILE.(OUT,RQU,1,MZE,MZE,ER9,ERCOD)                                            *141 LM00104    
                  STPCHG.                                                                         *142 LM00105    
                  TRANSFER TO SCMFD                                                               *143 LM00106    
                                                                                                       LM00107    
                 ...READ CONTROL CARDS FROM A2 IF SWT 4 IS DOWN                                        LM00108    
                                                                                                       LM00109    
      ENDMFD      UPDCHG.                                                                         *144 LM00110    
                  ONLINE.(MFDMSG...MFDMSG(6))                                                     *145 LM00111    
                  ONLINE.(MFDMS1...MFDMS1(5))                                                     *146 LM00112    
                        VECTOR VALUES MFDMSG = $FINISHED RQUEST FILES. SWT 4-DWN-TAPE A2$         *147 LM00113    
                     VECTOR VALUES MFDMS1 = $$,$$,$$,$SWT 4 -UP- QUIT$                            *148 LM00114    
                      VECTOR VALUES MFDMS2 = $SWT 1 - READ ONLINE$                                *149 LM00115    
                  PAUSE NO. 1                                                                     *150 LM00116    
                  WHENEVER .NOT. .SW. 4, TRANSFER TO QUIT                                         *151 LM00117    
      GOCARD      CT = 0                                                                          *152 LM00118    
                  ONLINE.(STRTA2...STRTA2(5))                                                     *153 LM00119    
                    VECTOR VALUES STRTA2 =$AM STARTING TAPE A2. RESET SWT 4.$                     *154 LM00120    
                    ONLINE.(MFDMS2...MFDMS2(3))                                                   *155 LM00121    
                  PAUSE NO. 2                                                                     *156 LM00122    
                                                                                                       LM00123    
                  TILOCK.(NXTNAM)                                                                 *157 LM00124    
      CRD1        TAPEI.(1002K)                                                                   *158 LM00125    
      CARD        WHENEVER .SW. 1                                                                 *159 LM00126    
                       RDCARD.(C(14)...RDCD)                                                      *160 LM00127    
                       C(1)=$$                                                                    *161 LM00128    
                       C(2)=$$                                                                    *162 LM00129    
                       T = 14                                                                     *163 LM00130    
                  OTHERWISE                                                                       *164 LM00131    
                   T = TAPE.(C(14)...C(1),ER8,CARD)                                               *165 LM00132    
                  END OF CONDITIONAL                                                              *166 LM00133    
                  WHENEVER .SW. 2, TRANSFER TO QUIT                                               *167 LM00134    
                  WHENEVER .SW. 4, TRANSFER TO RQUST                                              *168 LM00135    
      CARDON      FF = SCANI.(C(14),CARD)                                                         *169 LM00136    
                  ERR.(C(14)...C(1),ONOFRT,ISCONT)                                                *170 LM00137    
      ISCONT      WHENEVER FF .E. $ CLOSE$, TRANSFER TO RQUST                                     *171 LM00138    
                  WHENEVER FF .E. $  STOP$                                                        *172 LM00139    
      CRDEOF      STPCHG.                                                                         *173 LM00140    
                  UPDCHG.                                                                         *174 LM00141    
                     ONLINE.(MSGXXX...MSGXXX(3))                                                  *175 LM00142    
                     ONLINE.(MSGXX1...MSGXX1(2))                                                  *176 LM00143    
                     ONLINE.(MSGXX2...MSGXX2(3))                                                  *177 LM00144    
                     VECTOR VALUES MSGXXX = $SWT 2 -UP- CONTINUE $                                *178 LM00145    
                     VECTOR VALUES MSGXX1 = $SWT 2 -DWN- QUIT$                                    *179 LM00146    
                     VECTOR VALUES MSGXX2 = $SWT 4 -DWN- REQUEST FILES$                           *180 LM00147    
                     PAUSE NO. 70707                                                              *181 LM00148    
                     WHENEVER .SW. 4, TRANSFER TO RQUST                                           *182 LM00149    
                     WHENEVER .SW. 2, TRANSFER TO QUIT                                            *183 LM00150    
                     BSRA2.                                                                       *184 LM00151    
                     TRANSFER TO CRD1                                                             *185 LM00152    
                  END OF CONDITIONAL                                                              *186 LM00153    
                  WHENEVER FF .E. $     C$                                                        *187 LM00154    
                       WHENEVER CT .E. 0, TRANSFER TO NXTNAM                                      *188 LM00155    
                       TRANSFER TO CARD                                                           *189 LM00156    
                  END OF CONDITIONAL                                                              *190 LM00157    
      WHATI       THROUGH WHATI, FOR I = 1,1,I .G. CTL .OR. FF .E. CTL(I)                         *191 LM00158    
                  WHENEVER I .G. CTL, TRANSFER TO ER7                                             *192 LM00159    
                  CT = 0                                                                          *193 LM00160    
                  PROB = ADJPRB.( SCAN.(ER7),ER7)                                                 *194 LM00161    
                  PROG = SCAN.(ER7)                                                               *195 LM00162    
                  WHENEVER PROG .A. 777777777700K .E. $ 32760$,                                   *196 LM00163    
                1    PROG = (8-(PROG .A. 7K)) .V. $CMFL00$                                        *196 LM00164    
                  STPCHG.                                                                         *197 LM00165    
                  CHARGE.(PROB,PROG)                                                              *198 LM00166    
                  CHARG1.(CTLCHG(I))                                                              *199 LM00167    
                  WHENEVER FF .E. $UPDMFD$, TRANSFER TO CNTRL1                                    *200 LM00168    
                  ATTACH.(PROB,PROG,ER10,ERCOD)                                                   *201 LM00169    
                  PROB1 = PROB                                                                    *202 LM00170    
                  PROG1 = PROG                                                                    *203 LM00171    
                  WHENEVER FF .E. $ PRINT$ .OR. FF .E. $PRNDEL$,                                  *204 LM00172    
                1    NPRINT.(EJECT...EJECT)                                                       *204 LM00173    
                  GETTIM.(DATE,TIME)                                                              *205 LM00174    
      NXTNAM      FNAM1 = SCAN.(CARD)                                                             *206 LM00175    
                  FNAM2 = SCAN.(ER7)                                                              *207 LM00176    
                                                                                                       LM00177    
                 ...GO TO CONTROL FUNCTION                                                             LM00178    
      CNTRL1      SUCESS= CTLF(I).                                                                *208 LM00179    
                  UPDATE.                                                                         *209 LM00180    
                  STPCH1.                                                                         *210 LM00181    
                  WHENEVER FF .E. $ INPUT$, TRANSFER TO CARD                                      *211 LM00182    
                  TRANSFER TO NXTNAM                                                              *212 LM00183    
                                                                                                       LM00184    
      QUIT        ERR.(MSGEND...MSGEND(3),ONOFRT,THRU)                                            *213 LM00185    
                     VECTOR VALUES MSGEND = $FINISHED INPUT EDIT.$                                *214 LM00186    
      THRU        STPCHG.                                                                         *215 LM00187    
                  UPDCHG.                                                                         *216 LM00188    
                  ATTACH.(DSKEDT,FILE,ER11,ERCOD)                                                 *217 LM00189    
                  FNAM2 = FILE                                                                    *218 LM00190    
                  PROB = $$                                                                       *219 LM00191    
                  PROG = $$                                                                       *220 LM00192    
                  PROB1 = $$                                                                      *221 LM00193    
                  PROG1 = $$                                                                      *222 LM00194    
                  FSTATE.(CRYMAP,FILE,A(8)...8,CHKCAR)                                            *223 LM00195    
                  WHENEVER A(6) .NE. 1,                                                           *224 LM00196    
                1 CLOSE.(CRYMAP,FILE,ER11,ERCOD)                                                  *224 LM00197    
      THRU1          BPNCHI.(2201K)                                                               *225 LM00198    
                  BPUNCH.(STOPCD...STOPCD(13))                                                    *226 LM00199    
                  BPUNCH.(STOPCD(20)...STOPCD(7))                                                 *227 LM00200    
                     VECTOR VALUES STOPCD(7) = $$,$$,$$,$$,$$,$$,$$,$$,$$,$$,$$,$$,$$             *228 LM00201    
                     VECTOR VALUES STOPCD = $STOP  -END OF CARRY TAPE$,$$,$$,$$                   *229 LM00202    
                     VECTOR VALUES STOPCD(20) = $CLOSE$                                           *230 LM00203    
                     BINEOF.(2201K)                                                               *231 LM00204    
                     BINEOF.(2201K)                                                               *232 LM00205    
                  RUNCRY.                                                                         *233 LM00206    
                  NPRINT.(EJECT...EJECT)                                                          *234 LM00207    
                  NPRINT.(CRYMSG...CRYMSG(5))                                                     *235 LM00208    
                     VECTOR VALUES CRYMSG = $ **TABLE OF CONTENTS OF CARRY TAPE.$                 *236 LM00209    
                  FNAM1 = CRYMAP                                                                  *237 LM00210    
                  PRNT.                                                                           *238 LM00211    
                  DELFIL.(CRYMAP,FILE,THRU2)                                                      *239 LM00212    
      THRU2       NPRINT.(EJECT...EJECT)                                                          *240 LM00213    
                  NPRINT.(REQUES...REQUES(3))                                                     *241 LM00214    
                     VECTOR VALUES REQUES = $ **REQUESTS PROCESSED$                               *242 LM00215    
                  FNAM1 = STAT                                                                    *243 LM00216    
                  CLOSE.(STAT,FILE,THRU4)                                                         *244 LM00217    
      THRU4       PRNT.                                                                           *245 LM00218    
                  DELFIL.(STAT,FILE,THRU3)                                                        *246 LM00219    
      THRU3       PRNEOF.                                                                         *247 LM00220    
                  CLOSE.($M.F.D.$,$(FILE)$,NX1)                                                   *248 LM00221    
      NX1         WHENEVER B4SWT .NE. 0                                                           *249 LM00222    
                     PNCEOF.                                                                      *250 LM00223    
                     BPNCHI.(2202K)                                                               *251 LM00224    
                     BINEOF.(2202K)                                                               *252 LM00225    
                     BINEOF.(2202K)                                                               *253 LM00226    
                     RUNCRY.                                                                      *254 LM00227    
                  END OF CONDITIONAL                                                              *255 LM00228    
                  WHENEVER .SW. 3                                                                 *256 LM00229    
                     ONLINE.(RES3...RES3(1))                                                      *257 LM00230    
                        VECTOR VALUES RES3 = $RESET SWT 3.$                                       *258 LM00231    
                     PAUSE NO. 10                                                                 *259 LM00232    
                  END OF CONDITIONAL                                                              *260 LM00233    
                  CARSWT = 0                                                                      *261 LM00234    
                                                                                                       LM00235    
                      FINISH OFF ASCII REQUESTS, IF ANY                                                LM00236    
                                                                                                       LM00237    
                  EXIT.                                                                           *262 LM00238    
                                                                                                       LM00239    
      CHKCAR      WHENEVER CARSWT .E. 77777K, TRANSFER TO THRU1                                   *263 LM00240    
                  TRANSFER TO THRU2                                                               *264 LM00241    
      RQUST       ONLINE.(TTAP...TTAP(6))                                                         *265 LM00242    
                  VECTOR VALUES TTAP = $AM STARTING REQUEST FILES. RESET SWT 4.$                  *266 LM00243    
                  BSRA2.                                                                          *267 LM00244    
                  PAUSE NO. 3                                                                     *268 LM00245    
                  TRANSFER TO RQST                                                                *269 LM00246    
                                                                                                       LM00247    
                 ...ERROR RETURNS                                                                      LM00248    
                                                                                                       LM00249    
      ER1         MSG1(4) = PROB                                                                  *270 LM00250    
                  MSG1(5) = PROG                                                                  *271 LM00251    
                  ERR.(MSG1...MSG1(5),ONOFRT,EOF)                                                 *272 LM00252    
                     VECTOR VALUES MSG1 = $CAN'T PROCESS RQUEST FILES$                            *273 LM00253    
      ER3         MSG3(5) = PROB                                                                  *274 LM00254    
                  MSG3(6) = PROG                                                                  *275 LM00255    
                  ERR.(MSG3...MSG3(6),ONOFRT,EOF)                                                 *276 LM00256    
                     VECTOR VALUES MSG3 = $ERR IN READING RQUEST FILES$                           *277 LM00257    
      ER4         ERR.(MSG4...MSG4(3),ONOFRT,READ)                                                *278 LM00258    
                     VECTOR VALUES MSG4 = $ILLEGAL RQUEST FORMAT$                                 *279 LM00259    
      ER5         ERR.(MSG5...MSG5(4), ONOFRT,READ)                                               *280 LM00260    
                     VECTOR VALUES MSG5 = $NOT LEGAL FROM RQUEST FILES$                           *281 LM00261    
      ER6         ERR.(MSG6...MSG6(3),ONOFRT,READ)                                                *282 LM00262    
                     VECTOR VALUES MSG6 = $ILLEGAL CONTROL WORD$                                  *283 LM00263    
      ER7         CT = 7777K                                                                      *284 LM00264    
                  ERR.(MSG7...MSG7(3),ONOFRT,ERRTST)                                              *285 LM00265    
                     VECTOR VALUES MSG7 = $ILLEGAL CARD FORMAT$                                   *286 LM00266    
      ER8         ERR.(MSG8...MSG8(3),ONOFRT,CARDON)                                              *287 LM00267    
                     VECTOR VALUES MSG8 = $UNRECOVERED ERR ON A2$                                 *288 LM00268    
      ER9         ERR.(MSG9...MSG9(3),ONOFRT,SCMFD)                                               *289 LM00269    
                     VECTOR VALUES MSG9 = $CAN'T DELETE RQUEST FILE$                              *290 LM00270    
      ER10        ERR.(MSG10...MSG10(2), ONOFRT,ERRTST)                                           *291 LM00271    
                     VECTOR VALUES MSG10 = $USER NOT IN MFD$                                      *292 LM00272    
      ER11        ERR.(MSG11...MSG11(5),ONOFRT,THRU1)                                             *293 LM00273    
                     VECTOR VALUES MSG11 = $CARRY TABLE OF CONTENTS TROUBLE$                      *294 LM00274    
      ER12        ERR.(MSG12...MSG12(3),ONOFRT,SCMFD)                                             *295 LM00275    
                     VECTOR VALUES MSG12 = $CAN'T CLOSE RQUEST FILE$                              *296 LM00276    
      ER13        ONLINE.(MSG13...MSG13(7))                                                       *297 LM00277    
                     VECTOR VALUES MSG13 = $CAN'T OPEN STATISTICS FILE. RERUN OR SALVAG.$         *298 LM00278    
                  CLOSE.(STAT,FILE,THRU3)                                                         *299 LM00279    
                  TRANSFER TO ALRDY1                                                              *300 LM00280    
      ER14        ERR.(MSG14...MSG14(2),ONOFRT,SCMFD)                                             *301 LM00281    
                     VECTOR VALUES MSG14 = $NO DAEMON REQUESTS$                                   *302 LM00282    
      ERRTST      WHENEVER FF .NE. $ INPUT$, TRANSFER TO CARD                                     *303 LM00283    
      ERRTT       TAPE.(C(14)...C(13),ERRTT,CARD)                                                 *304 LM00284    
                  TRANSFER TO ERRTT                                                               *305 LM00285    
                                                                                                       LM00286    
                 ...PROGRAM DATA                                                                       LM00287    
                                                                                                       LM00288    
                  DIMENSION A(9), BUFFR(470),BUFT(432)                                            *306 LM00289    
                  VECTOR VALUES ERCOD = 0                                                         *307 LM00290    
                  VECTOR VALUES DAEPRB = $ C0056$                                                 *308 LM00291    
                  VECTOR VALUES DAEPRG = $ 99999$                                                 *309 LM00292    
                  VECTOR VALUES B4SWT = 0                                                         *310 LM00293    
                  VECTOR VALUES PRNSWT = 0                                                        *311 LM00294    
                  VECTOR VALUES STAT = $STATIS$                                                   *312 LM00295    
                  VECTOR VALUES FF = 0                                                            *313 LM00296    
                  VECTOR VALUES T = 0                                                             *314 LM00297    
