                  VECTOR VALUES DSKEDT = $DSKEDT$                                                 *058 LM00015    
                  VECTOR VALUES FILE = $.FILE.$                                                   *059 LM00016    
                  VECTOR VALUES CRYMAP = $CRYMAP$                                                 *060 LM00017    
                  NORMAL MODE IS INTEGER                                                          *061 LM00018    
                                                                                                       LM00008    
                  SEQ = 0                                                                         *062 LM00009    
                  MODE = 0                                                                        *063 LM00010    
                  FSTATE.(FNAM1,FNAM2,T(8)...8, TRBL, ERCOD)                                      *064 LM00011    
                  MAY NOT INPUT FILE OF SAME NAME UNLESS PRIVILEGED                                    LM00012    
                  WHENEVER PRIVIL .E. 0                                                           *065 LM00013    
                     WHENEVER T(7) .A. 120K .NE. 0, TRANSFER TO ER23                              *066 LM00014    
                     WHENEVER (T(7).RS.35) .NE. 0, TRANSFER TO ER23                               *067 LM00015    
                  END OF CONDITIONAL                                                              *068 LM00016    
                  MODE = T(7)                                                                     *069 LM00017    
      WHTMD       THROUGH NAMFND, FOR J = 1,1, J .E. 10                                           *070 LM00018    
                  FSTATE.(TEMPF1,TEMPF2,T(8)...8,OPN1)                                            *071 LM00019    
      NAMFND      TEMPF2 = TEMPF2(J)                                                              *072 LM00020    
                  TRANSFER TO ER20                                                                *073 LM00021    
      OPN1        MODE = SCAN.(OPN)                                                               *074 LM00022    
                  MODE = SOCTBN.(MODE)                                                            *075 LM00023    
                  WHENEVER MODE .E. 1, MODE = 0                                                   *076 LM00024    
      OPN         OPEN.($W$,TEMPF1,TEMPF2,1,MZE,ER20,ERCOD)                                       *077 LM00025    
                  BUFFER.(TEMPF1,TEMPF2,BUF(432)...432,ER20,ERCOD)                                *078 LM00026    
                  PASSIS = 1B                                                                     *079 LM00027    
                  J = 432                                                                         *080 LM00028    
                  CNT = 0                                                                         *081 LM00029    
      INP1        T = TAPE.(T(28)...T(1),RTT,EOF)                                                 *082 LM00030    
                  WHENEVER PASSIS                                                                 *083 LM00031    
                  IGNORE FLIP CARDS                                                                    LM00032    
                  WHENEVER T(28) .A. 77777K6 .E. 5K8,                                             *084 LM00033    
                1    TRANSFER TO INP1                                                             *084 LM00034    
                       PASSIS = 0B                                                                *085 LM00035    
                     WHENEVER T(28) .A. 7007K8 .E. 7005K8, TRANSFER TO INP7A                      *086 LM00036    
                  END OF CONDITIONAL                                                              *087 LM00037    
                  I = 1                                                                           *088 LM00038    
      LPP         THROUGH LLP, FOR CNT = CNT,1,CNT .GE. 432 .OR. I .G. T                          *089 LM00039    
                  S(J-CNT) = T(29-I)                                                              *090 LM00040    
      LLP         I = I+1                                                                         *091 LM00041    
                  WHENEVER CNT .GE. 432                                                           *092 LM00042    
                     WRFILE.(TEMPF1,TEMPF2,0,S(J)...432,MZE,MZE,ER21,ERCOD)                       *093 LM00043    
                     WHENEVER J .G. 433                                                           *094 LM00044    
                        J = 432                                                                   *095 LM00045    
                     OTHERWISE                                                                    *096 LM00046    
                        J = 900                                                                   *097 LM00047    
                     END OF CONDITIONAL                                                           *098 LM00048    
                     CNT = 0                                                                      *099 LM00049    
                     TRANSFER TO LPP                                                              *100 LM00050    
                  END OF CONDITIONAL                                                              *101 LM00051    
                  TRANSFER TO INP1                                                                *102 LM00052    
      RTT         ERR.(RTTMSG...RTTMSG(5),ONOFRT,ENDI)                                            *103 LM00053    
                     VECTOR VALUES RTTMSG = $REDUNDANCY ON TAPE. FILE NOT INPUT.$                 *104 LM00054    
                                                                                                       LM00055    
      EOF         WHENEVER CNT .NE. 0,                                                            *105 LM00056    
                1    WRFILE.(TEMPF1,TEMPF2,0,S(J)...CNT,MZE,MZE,ER21,ERCOD)                       *105 LM00057    
                  CLOSE.(TEMPF1,TEMPF2,ER24,ERCOD)                                                *106 LM00058    
                  CHFILE.(FNAM1,FNAM2,1,MZE,MZE,CHF)                                              *107 LM00059    
