                  WHENEVER PG .E. 0,PG = DECCNV.(SCAN.(ZER)).A.777777K                            *112 LM00064    
      ZER         T(1) = (T(1) .A. 777777K6) .V. PG                                               *113 LM00065    
                  SETFIL.(FNAM1,FNAM2,T(2),T(1),MODE,2,FLINK)                                     *114 LM00066    
                  DELFIL.(TEMPF1,TEMPF2,FLINK)                                                    *115 LM00067    
