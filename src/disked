                  DELFIL.(TEMPF1,TEMPF2,FAILED,ERCOD)                                             *121 LM00074    
      FAILED      FUNCTION RETURN 0                                                               *122 LM00075    
                                                                                                       LM00076    
      INP7        T = TAPE.(T(28)...T(1),RTT,EOF99)                                               *123 LM00077    
      INP7A       WHENEVER T(28) .A. 7007K8 .NE. 7005K8,                                          *124 LM00078    
                1  ERR.(ILGMSG...ILGMSG(6),ONOFRT,ENDI)                                           *124 LM00079    
                     VECTOR VALUES ILGMSG = $ILLEGAL CARD IN DECK. FILE NOT INPUT.$               *125 LM00080    
                  TPRIME = (T(28) .A. 77K6) .RS. 18                                               *126 LM00081    
                  WC = (T(28) .A. 77K9) .RS. 21                                                   *127 LM00082    
                  TPRIME = TPRIME .V. WC                                                          *128 LM00083    
                  WHENEVER TPRIME .E. 0, TRANSFER TO INP7                                         *129 LM00084    
                  WC = T(28) .A. 77777K                                                           *130 LM00085    
                  WHENEVER WC .NE. SEQ, TRANSFER TO ER25                                          *131 LM00086    
                  SEQ = SEQ +1                                                                    *132 LM00087    
                  TAG = T(28) .A. 7K5                                                             *133 LM00088    
      INPCON      WHENEVER TPRIME .LE. 22                                                         *134 LM00089    
                       WC = TPRIME                                                                *135 LM00090    
                  OTHERWISE                                                                       *136 LM00091    
                       WC = 1                                                                     *137 LM00092    
                  END OF CONDITIONAL                                                              *138 LM00093    
                  CKSUM = T(28)                                                                   *139 LM00094    
                  THROUGH INP7L, FOR I = 1,1,I .G. WC                                             *140 LM00095    
      INP7L       CKSUM = CKSUM .ACL. T(27-I)                                                     *141 LM00096    
                  WHENEVER CKSUM .NE. T(27)                                                       *142 LM00097    
                       CSMSG(4) = SPRDOC.(T(28) .RS. 18)                                          *143 LM00098    
                       CSMSG(5) = SPRDOC.(T(28) .A. 77777K)                                       *144 LM00099    
                       ERR.(CSMSG...CSMSG(8),ONOFRT,CKTEST)                                       *145 LM00100    
      CKTEST           WHENEVER T(27) .NE. 0, TRANSFER TO ENDI                                    *146 LM00101    
                  END OF CONDITIONAL                                                              *147 LM00102    
                     VECTOR VALUES CSMSG = $CHECK SUM ERROR ON CARD$,$$,$$,                       *148 LM00103    
                1  $ FILE NOT INPUT.$                                                             *148 LM00104    
                     I = 1                                                                        *149 LM00105    
      LPP2           THROUGH LLP2, FOR CNT = CNT,1,CNT .GE. 432 .OR.  I .G. TPRIME                *150 LM00106    
                  WHENEVER WC .E. 1                                                               *151 LM00107    
                        S(J-CNT) = T(26)                                                          *152 LM00108    
                     OTHERWISE                                                                    *153 LM00109    
                        S(J-CNT) = T(27-I)                                                        *154 LM00110    
                     END OF CONDITIONAL                                                           *155 LM00111    
      LLP2           I = I+1                                                                      *156 LM00112    
