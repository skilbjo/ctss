                  EXTERNAL FUNCTION(ISRT,REV,WORD,POINT)                                          *001 LM00008    
                                                                                                       LM00009    
                      ISRT=0 - FILE DIRECTORY ORDER                                                    LM00010    
                           1 - ALPHABETICALLY (NAME1,NAME2)                                            LM00011    
                           2 - ALPHABETICALLY (NAME2 NAME1)                                            LM00012    
                           3 - NUMERICALLY (MODE,MADE)                                                 LM00013    
                           4 - NUMERICALLY (LENGTH,MADE)                                               LM00014    
                           5 - CHRONOLOGICALLY (USED,MADE)                                             LM00015    
                           6 - CHRONOLOGICALLY (MADE)                                                  LM00016    
                                                                                                       LM00017    
                      REV=(+) - LARGEST FIRST                                                          LM00018    
                          (-) - SMALLEST FIRST                                                         LM00019    
                                                                                                       LM00020    
                      WORD = ARRAY CONTAINING FILE DIRECTORY TO BE SORTED                              LM00021    
                                                                                                       LM00022    
                      POINT(0)=NUMBER OF ENTRIES TO BE SORTED                                          LM00023    
                      POINT(1)...POINT(POINT(0))=POINTERS TO ACCEPTABLE FILES                          LM00024    
                                                                                                       LM00025    
                                                                                                       LM00026    
                  NORMAL MODE IS INTEGER                                                          *002 LM00027    
                                                                                                       LM00028    
                      (OPERATOR) - LOGICALLY GREATER THAN, .LG.                                        LM00029    
                                                                                                       LM00030    
                                                                                                       LM00031    
                  DEFINE BINARY OPERATOR .LG., PRECEDENCE SAME AS .G.                             *003 LM00032    
                  MODE STRUCTURE 1 .LG. 1 TO 2                                                    *004 LM00033    
             JMP  *+1,AC,*+2                                                                      *005 LM00034    
             STO  T                                                                               *006 LM00035    
             JMP  *+1,LA,*+2                                                                      *007 LM00036    
             SLW  T                                                                               *008 LM00037    
             CAL  A                                                                               *009 LM00038    
             LAS  B                                                                               *010 LM00039    
             TRA  LOC+3                                                                           *011 LM00040    
             AXT  0,0                                                                             *012 LM00041    
             PXD  0                                                                               *013 LM00042    
             OUT  AC                                                                              *014 LM00043    
             END                                                                                  *015 LM00044    
                                                                                                       LM00045    
                                                                                                       LM00046    
                                                                                                       LM00047    
                      (OPERATOR) - LOGICALLY EXCHANGE, .XCL.                                           LM00048    
                                                                                                       LM00049    
                  DEFINE BINARY OPERATOR .XCL., PRECEDENCE SAME AS .G.                            *016 LM00050    
                  MODE STRUCTURE 1 .XCL. 1 TO 1                                                   *017 LM00051    
                                                                                                       LM00052    
             CAL  A                                                                               *018 LM00053    
             LDQ  B                                                                               *019 LM00054    
             STQ  A                                                                               *020 LM00055    
             SLW  B                                                                               *021 LM00056    
             OUT  ACQ                                                                             *022 LM00057    
             END                                                                                  *023 LM00058    
                                                                                                       LM00059    
                                                                                                       LM00060    
                                                                                                       LM00061    
                      (PROCEDURE) - ADJUST WORDS FOR ALPHABETIC SORT                                   LM00062    
                                                                                                       LM00063    
                      POINT(I)3-5=NO. OF LEADING BLANKS IN NAME1(I)                                    LM00064    
                      POINT(I)6-8=NO. OF LEADING BLANKS IN NAME2(I)                                    LM00065    
                                                                                                       LM00066    
                  INTERNAL FUNCTION (REL)                                                         *024 LM00067    
                                                                                                       LM00068    
                                                                                                       LM00069    
                      (ENTRY) - REMOVE LEADING BLANKS AND LEFT ADJUST WORDS                            LM00070    
                                                                                                       LM00071    
                  ENTRY TO SET.                                                                   *025 LM00072    
                                                                                                       LM00073    
                  SHIFT=30-REL*3                                                                  *026 LM00074    
                  THROUGH SETL,FOR I=1,1,I.G.NP                                                   *027 LM00075    
                  P=POINT(I)-REL                                                                  *028 LM00076    
                  XI=WORD(P)                                                                      *029 LM00077    
                  THROUGH BLANKS,FOR J=0,1,J.E.6                                                  *030 LM00078    
                  WHENEVER (XI.A.770000000000K).NE.BLANK(1)                                       *031 LM00079    
                       POINT(I)=POINT(I).V.(J.LS.SHIFT)                                           *032 LM00080    
                       WORD(P)=XI                                                                 *033 LM00081    
                       TRANSFER TO SETL                                                           *034 LM00082    
                  OTHERWISE                                                                       *035 LM00083    
                       XI=XI.LS.6                                                                 *036 LM00084    
                  END OF CONDITIONAL                                                              *037 LM00085    
      BLANKS      CONTINUE                                                                        *038 LM00086    
      SETL        CONTINUE                                                                        *039 LM00087    
                  FUNCTION RETURN                                                                 *040 LM00088    
                                                                                                       LM00089    
                      WORDS READY TO BE SORTED                                                         LM00090    
                                                                                                       LM00091    
                                                                                                       LM00092    
                                                                                                       LM00093    
                      (ENTRY) - RIGHT ADJUST WORDS AND REPLACE LEADING BLANKS                          LM00094    
                                                                                                       LM00095    
                  ENTRY TO UNSET.                                                                 *041 LM00096    
                  SHIFT=30-REL*3                                                                  *042 LM00097    
                  THROUGH UNSETL,FOR I=1,1,I.G.NP                                                 *043 LM00098    
                  P=POINT(I)-REL                                                                  *044 LM00099    
                  XJ=(POINT(I).RS.SHIFT).A.7K                                                     *045 LM00100    
                  WORD(P)=(WORD(P).RS.(6*XJ)).V.BLANK(XJ)                                         *046 LM00101    
      UNSETL      CONTINUE                                                                        *047 LM00102    
                  FUNCTION RETURN                                                                 *048 LM00103    
                                                                                                       LM00104    
                      WORDS RESTORED                                                                   LM00105    
                                                                                                       LM00106    
                  END OF FUNCTION                                                                 *049 LM00107    
                                                                                                       LM00108    
                                                                                                       LM00109    
                                                                                                       LM00110    
                      (PROCEDURE) - EXCHANGE SORT, LARGEST ITEM FIRST                                  LM00111    
                                                                                                       LM00112    
                  INTERNAL FUNCTION (REL,MSK)                                                     *050 LM00113    
                                                                                                       LM00114    
                                                                                                       LM00115    
                  ENTRY TO SORTS.                                                                 *051 LM00116    
                                                                                                       LM00117    
                  P=NP+1                                                                          *052 LM00118    
                  THROUGH SORT,FOR I=1,1,I.E.NP                                                   *053 LM00119    
                  XCHNGE=0                                                                        *054 LM00120    
                  XI=WORD(POINT(1)-REL).A.MSK                                                     *055 LM00121    
                  THROUGH SORT1,FOR J=2,1,J.E.P                                                   *056 LM00122    
                  XJ=WORD(POINT(J)-REL).A.MSK                                                     *057 LM00123    
                  WHENEVER XJ.LG.XI                                                               *058 LM00124    
                       XCHNGE=1                                                                   *059 LM00125    
                       POINT(J).XCL.POINT(J-1)                                                    *060 LM00126    
                  OTHERWISE                                                                       *061 LM00127    
                       XI=XJ                                                                      *062 LM00128    
                  END OF CONDITIONAL                                                              *063 LM00129    
      SORT1       CONTINUE                                                                        *064 LM00130    
                  WHENEVER XCHNGE.E.0,FUNCTION RETURN                                             *065 LM00131    
                                                                                                       LM00132    
                      CONTINUE SORTING                                                                 LM00133    
                  P=P-1                                                                           *066 LM00134    
      SORT        CONTINUE                                                                        *067 LM00135    
                  FUNCTION RETURN                                                                 *068 LM00136    
                                                                                                       LM00137    
                      FINISHED SORTING                                                                 LM00138    
                                                                                                       LM00139    
                                                                                                       LM00140    
                  END OF FUNCTION                                                                 *069 LM00141    
                                                                                                       LM00142    
                                                                                                       LM00143    
                                                                                                       LM00144    
