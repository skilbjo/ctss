                      PRESET DECLARATIONS.                                                             LM00020    
                                                                                                       LM00021    
                  VECTOR VALUES  C0056 = $ C0056$                                                 *005 LM00022    
                  VECTOR VALUES FBPROG = $ 99995$                                                 *006 LM00023    
                  VECTOR VALUES FIBJOB = $FIBJOB$                                                 *007 LM00024    
