                  UNLINK. (FIBJOB, FILE)                                                          *106 LM00175    
                                                                                                       LM00176    
                      READ THRU FIBJOB FILE, SEE IF HE HAS A JOB ALREADY.                              LM00177    
                                                                                                       LM00178    
      LOOP        BFREAD. (FIBJOB, FILE, ENTRY (13) ... 14, ENDRD, C, ERROR)                      *107 LM00179    
                  WHENEVER TPROB .E. PROB .AND. TPROG .E. PROG                                    *108 LM00180    
                       DELTA = 60 * (ABSTIM. (TTIME) - ABSTIM. (DRB4) )                           *109 LM00181    
                       WHENEVER .ABS. DELTA .L. TIMSLT, TRANSFER TO DEFER                         *110 LM00182    
                  END OF CONDITIONAL                                                              *111 LM00183    
                  TRANSFER TO LOOP                                                                *112 LM00184    
                                                                                                       LM00185    
      ENDRD       BUFFER. (FIBJOB, FILE, BUFF (431) ... 432)                                      *113 LM00186    
                  WRFILE. (FIBJOB, FILE, 0, CARD (13) ... 14)                                     *114 LM00187    
                  CLOSE. (FIBJOB, FILE)                                                           *115 LM00188    
                                                                                                       LM00189    
                      PRINT COMMENT IF JOB WAS DEFERRED.                                               LM00190    
                                                                                                       LM00191    
                  WHENEVER DEFSW                                                                  *116 LM00192    
                       FTIME. (DRB4, DEFCOM (8), DEFCOM (9))                                      *117 LM00193    
                       DEFCOM (8) = BCDATE. (DEFCOM (8))                                          *118 LM00194    
                       PRNTP. (DEFCOM)                                                            *119 LM00195    
                       VECTOR VALUES DEFCOM = $ EXECUTION OF YOUR JOB HAS BEEN DEFERRED U         *120 LM00196    
                1 NTIL $,                                                                         *120 LM00197    
                2      0, 0, 777777777777K                                                        *120 LM00198    
                  END OF CONDITIONAL                                                              *121 LM00199    
                                                                                                       LM00200    
      CHN         CHNCOM. (0)                                                                     *122 LM00201    
                                                                                                       LM00202    
                                                                                                       LM00203    
                      DEFER THIS JOB FOR ONE HOUR.                                                     LM00204    
                                                                                                       LM00205    
      DEFER       HOUR = HOUR + TIMSLT + DELTA                                                    *123 LM00206    
                  BFCLOS. (FIBJOB, FILE, ERROR)                                                   *124 LM00207    
                  DEFSW = 1B                                                                      *125 LM00208    
                  TRANSFER TO SETTIM                                                              *126 LM00209    
