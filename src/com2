                                                                                                       LM00068    
                      ****** PARAMETER CHECK AND SETUP. ******                                         LM00069    
                                                                                                       LM00070    
                  FERRTN. (ERROR)                                                                 *045 LM00071    
                  WHOAMI.(ME(6)...7)                                                              *046 LM00072    
                  DIMENSION ME(6)                                                                 *047 LM00073    
                  PROB = ME(6)                                                                    *048 LM00074    
                  PROG = ME(5)                                                                    *049 LM00075    
                  NAME = ME(0)                                                                    *050 LM00076    
                  LUN = GLOC.(COMLOC.($USER$))                                                    *051 LM00077    
                                                                                                       LM00078    
                  FNAM = COMARG. (1)                                                              *052 LM00079    
                  WHENEVER FNAM .E. FENCE                                                         *053 LM00080    
                       PRNTP. (USAGE)                                                             *054 LM00081    
                       VECTOR VALUES USAGE = $ USAGE IS  'FIB  JOBNAM  -TIMLIM- $,                *055 LM00082    
                1           $-TIME-  -DAY-'.$,                                                    *055 LM00083    
                1           777777777777K                                                         *055 LM00084    
                       TRANSFER TO CHN                                                            *056 LM00085    
                  END OF CONDITIONAL                                                              *057 LM00086    
                                                                                                       LM00087    
                  WHENEVER GLOC.(COMLOC.($TAU5$)+LUN) .RS. 22 .E. 0                               *058 LM00088    
                       PRNTP. (ILLEG)                                                             *059 LM00089    
                       VECTOR VALUES ILLEG = $ USER MUST HAVE SHIFT 5 TIME QUOTA$,                *060 LM00090    
                1      $IN ORDER TO USE FIB COMMAND.$,                                            *060 LM00091    
