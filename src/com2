                       TRANSFER TO CHN                                                            *061 LM00093    
                  END OF CONDITIONAL                                                              *062 LM00094    
                                                                                                       LM00095    
                                                                                                       LM00096    
                      CHECK FOR EXISTENCE OF BCD FILE.                                                 LM00097    
                                                                                                       LM00098    
                  FSTATE.(FNAM,RUNCOM,LTH...1,NEXT)                                               *063 LM00099    
                  TRANSFER TO ONWARD                                                              *064 LM00100    
      NEXT        FSTATE. (FNAM, BCD, LTH ... 1, NOFILE)                                          *065 LM00101    
      ONWARD      WHENEVER LTH .E. 0, TRANSFER TO NOFILE                                          *066 LM00102    
                                                                                                       LM00103    
                                                                                                       LM00104    
                      GET DATE AND TIME NOW.                                                           LM00105    
                                                                                                       LM00106    
                  DRB4 = GTDYTM. (0)                                                              *067 LM00107    
                  YEAR = DRB4 .RS. 27                                                             *068 LM00108    
                  MONTH = (DRB4 .RS. 23) .A. 17K                                                  *069 LM00109    
                  TODAY = (DRB4 .RS. 18) .A. 37K                                                  *070 LM00110    
                  NWHOUR = DRB4 .A. 777777K                                                       *071 LM00111    
                  WHENEVER YEAR .MOD. 4 .E. 0, DAYS (2) = 29                                      *072 LM00112    
                                                                                                       LM00113    
                                                                                                       LM00114    
                      CHECK TIME PARAMETERS.                                                           LM00115    
                                                                                                       LM00116    
                  LIM = COMARG. (2)                                                               *073 LM00117    
                  WHENEVER LIM .E. FENCE                                                          *074 LM00118    
                       LIM = DFAULT                                                               *075 LM00119    
                       HOUR = NWHOUR                                                              *076 LM00120    
                       DAY = TODAY                                                                *077 LM00121    
                                                                                                       LM00122    
                  OTHERWISE                                                                       *078 LM00123    
                       LIM = BCDEC. (LIM)                                                         *079 LM00124    
                       WHENEVER (PROB.NE.$ M1416$ .AND. PROB.NE.$ C0032$)                         *080 LM00125    
                1        .AND. LIM.G.TIMLIM, LIM = TIMLIM                                         *080 LM00126    
                                                                                                       LM00127    
                       WHENEVER COMARG. (3, HOUR) .E. FENCE                                       *081 LM00128    
                            HOUR = NWHOUR                                                         *082 LM00129    
                            DAY = TODAY                                                           *083 LM00130    
                                                                                                       LM00131    
                       OTHERWISE                                                                  *084 LM00132    
                            MIN = BCDEC. (HOUR .A. 7777K)                                         *085 LM00133    
                            WHENEVER MIN .GE. 60, TRANSFER TO BADATE                              *086 LM00134    
                            HOUR = BCDEC. (HOUR .RS. 12)                                          *087 LM00135    
                            WHENEVER HOUR .GE. 24, TRANSFER TO BADATE                             *088 LM00136    
                            HOUR = ((HOUR * 60) + MIN) * 60                                       *089 LM00137    
                                                                                                       LM00138    
                            WHENEVER COMARG. (4, DAY) .E. FENCE                                   *090 LM00139    
                                 DAY = TODAY                                                      *091 LM00140    
                                 WHENEVER HOUR .L. NWHOUR, DAY = DAY + 1                          *092 LM00141    
                                                                                                       LM00142    
                            OTHERWISE                                                             *093 LM00143    
                                 DAY = BCDEC. (DAY)                                               *094 LM00144    
                                 WHENEVER DAY .L. TODAY .OR.                                      *095 LM00145    
                1                     (DAY .E. TODAY .AND. HOUR .L. NWHOUR),                      *095 LM00146    
                2                     MONTH = MONTH + 1                                           *095 LM00147    
                                 WHENEVER DAY .G. DAYS (MONTH), TRANSFER TO BADATE                *096 LM00148    
                                                                                                       LM00149    
                            END OF CONDITIONAL                                                    *097 LM00150    
                                                                                                       LM00151    
                       END OF CONDITIONAL                                                         *098 LM00152    
                                                                                                       LM00153    
                  END OF CONDITIONAL                                                              *099 LM00154    
                                                                                                       LM00155    
                                                                                                       LM00156    
                                                                                                       LM00157    
      SETTIM      CHKTIM. (HOUR, 86400, DAY)                                                      *100 LM00158    
                  CHKTIM. (DAY, DAYS (MONTH), MONTH)                                              *101 LM00159    
                  CHKTIM. (MONTH, 12, YEAR)                                                       *102 LM00160    
