                  VECTOR VALUES FIBMON = $ 99995$                                                 *032 LM00048    
                  VECTOR VALUES     RW = $RW$                                                     *033 LM00049    
                  VECTOR VALUES      C = 0                                                        *034 LM00050    
                  VECTOR VALUES    MZE = -0                                                       *035 LM00051    
                  VECTOR VALUES TIMLIM = 10                                                       *036 LM00052    
                  VECTOR VALUES DFAULT = 5                                                        *037 LM00053    
                  VECTOR VALUES  FENCE = 777777777777K                                            *038 LM00054    
                  VECTOR VALUES TIMSLT = 3600                                                     *039 LM00055    
                  VECTOR VALUES  DEFSW = 0B                                                       *040 LM00056    
                  VECTOR VALUES   DAYS = 0, 31, 28, 31, 30, 31, 30, 31, 31, 30, 31,               *041 LM00057    
                1      30, 31                                                                     *041 LM00058    
                                                                                                       LM00059    
                  VECTOR VALUES CARD ... CARD (13) = $$                                           *042 LM00060    
                  EQUIVALENCE  (PROB, CARD (13)), (NAME, CARD (12)),                              *043 LM00061    
                1      (PROG, CARD (11)), (FNAM, CARD (10)),                                      *043 LM00062    
                2      ( LIM, CARD ( 9)), (DRB4, CARD ( 8))                                       *043 LM00063    
                                                                                                       LM00064    
                  EQUIVALENCE  (TPROB, ENTRY (13)), (TPROG, ENTRY (11)),                          *044 LM00065    
                1      (TJOB , ENTRY (10)), (TTIME, ENTRY ( 8))                                   *044 LM00066    
