                 ..  ROUTINE TO GET DEVICE NUMBER OF TYPEWRITER CONSOLE.                               LM00001    
                 ..  J. H. SALTZER, DECEMBER, 1966.                                                    LM00002    
                                                                                                       LM00003    
                  EXTERNAL FUNCTION(DEVICE, PLTH)                                                 *001 LM00004    
                  NORMAL MODE IS INTEGER                                                          *002 LM00005    
                  VECTOR VALUES M10LTH = 130                                                      *003 LM00006    
                  VECTOR VALUES M27LTH = 120                                                      *004 LM00007    
                  VECTOR VALUES M35LTH = 84                                                       *005 LM00008    
                  VECTOR VALUES M37LTH = 84                                                       *006 LM00009    
                  VECTOR VALUES M37 = 1                                                           *007 LM00010    
                  VECTOR VALUES M1050C = 2                                                        *008 LM00011    
                  VECTOR VALUES M2741E = 3                                                        *009 LM00012    
                  VECTOR VALUES M35 = 4                                                           *010 LM00013    
                  DIMENSION GLOP(4)                                                               *011 LM00014    
                                                                                                       LM00015    
                  ENTRY TO GETDEV.                                                                *012 LM00016    
                  WHOAMI.(GLOP(4)...4)                                                            *013 LM00017    
                  JUNK = GLOP(1) .RS. 30                                                          *014 LM00018    
                  WHENEVER JUNK .E. 1 .OR. JUNK .E. 4 .OR. JUNK .E. 6                             *015 LM00019    
                     DEVICE = M35                                                                 *016 LM00020    
                     PLTH = M35LTH                                                                *017 LM00021    
                  OR WHENEVER JUNK .E. 2                                                          *018 LM00022    
                     DEVICE = M2741E                                                              *019 LM00023    
                     PLTH = M10LTH                                                                *020 LM00024    
                  OR WHENEVER JUNK .E. 8                                                          *021 LM00025    
                     PLTH = M27LTH                                                                *022 LM00026    
                     DEVICE = M2741E                                                              *023 LM00027    
                  OR WHENEVER JUNK .E. 7                                                          *024 LM00028    
                     DEVICE = M37                                                                 *025 LM00029    
                     PLTH = M37LTH                                                                *026 LM00030    
                  OTHERWISE                                                                       *027 LM00031    
                     DEVICE = M37                                                                 *028 LM00032    
                     PLTH = 80                                                                    *029 LM00033    
                  END OF CONDITIONAL                                                              *030 LM00034    
                  FUNCTION RETURN                                                                 *031 LM00035    
                  END OF FUNCTION                                                                 *032 LM00036    
