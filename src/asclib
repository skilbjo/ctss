                 ..  ERASE AND KILL EDITOR FOR ASCII FILES.                                            LM00001    
                 ..  FIXED FOR PR(1966) ASCII.                                                         LM00002    
                 ..  J. H. SALTZER, JANUARY, 1966                                                      LM00003    
                                                                                                       LM00004    
                      USAGE. . .                                                                       LM00005    
                 ..          EKEDIT.(LINE, INSIZE, SIZE)                                               LM00006    
                                                                                                       LM00007    
                  EXTERNAL FUNCTION(LINE, INSIZE, SIZE)                                           *001 LM00008    
                                                                                                       LM00009    
                  NORMAL MODE IS INTEGER                                                          *002 LM00010    
                  BOOLEAN TRAIL                                                                   *003 LM00011    
                  VECTOR VALUES SPACE = 040K                                                      *004 LM00012    
                  VECTOR VALUES BACKSP = 010K                                                     *005 LM00013    
                  VECTOR VALUES HTAB = 011K                                                       *006 LM00014    
                  VECTOR VALUES MCNG = 033K                                                       *007 LM00015    
                  VECTOR VALUES ERASE = 043K                                                      *008 LM00016    
                  VECTOR VALUES KILL = 100K                                                       *009 LM00017    
                  VECTOR VALUES LILE = 145K                                                       *010 LM00018    
                  VECTOR VALUES CAPE = 105K                                                       *011 LM00019    
                  VECTOR VALUES LILK = 153K                                                       *012 LM00020    
                  VECTOR VALUES CAPK = 113K                                                       *013 LM00021    
                                                                                                       LM00022    
                  ENTRY TO SETERS.                                                                *014 LM00023    
                    ERASE = LINE                                                                  *015 LM00024    
                    FUNCTION RETURN                                                               *016 LM00025    
                                                                                                       LM00026    
                  ENTRY TO SETKIL.                                                                *017 LM00027    
                    KILL = LINE                                                                   *018 LM00028    
                    FUNCTION RETURN                                                               *019 LM00029    
                                                                                                       LM00030    
                  ENTRY TO EKEDIT.                                                                *020 LM00031    
                    SIZE = 0                                                                      *021 LM00032    
                    THROUGH LLP, FOR I = 1, 1, I .G. INSIZE                                       *022 LM00033    
                      WHENEVER LINE(I) .E. ERASE                                                  *023 LM00034    
                        TRAIL = 1B                                                                *024 LM00035    
                        WHENEVER SIZE .E. 0, TRAIL = 0B                                           *025 LM00036    
      BTEST             WHENEVER LINE(SIZE) .E. BACKSP                                            *026 LM00037    
                          TRAIL = 0B                                                              *027 LM00038    
                          SIZE = SIZE - 2                                                         *028 LM00039    
                          TRANSFER TO BTEST                                                       *029 LM00040    
                        END OF CONDITIONAL                                                        *030 LM00041    
      FTEST             WHENEVER LINE(I+1) .E. BACKSP                                             *031 LM00042    
                          TRAIL = 0B                                                              *032 LM00043    
                          I = I + 2                                                               *033 LM00044    
                          TRANSFER TO FTEST                                                       *034 LM00045    
                        END OF CONDITIONAL                                                        *035 LM00046    
                        WHENEVER TRAIL                                                            *036 LM00047    
                          WHENEVER LINE(SIZE) .NE. SPACE                                          *037 LM00048    
                1          .AND. LINE(SIZE) .NE. HTAB                                             *037 LM00049    
                            SIZE = SIZE - 1                                                       *038 LM00050    
                            TRAIL = 0B                                                            *039 LM00051    
                            TRANSFER TO BTEST                                                     *040 LM00052    
                          END OF CONDITIONAL                                                      *041 LM00053    
      STEST               WHENEVER LINE(SIZE) .E. SPACE                                           *042 LM00054    
                1            .OR. LINE(SIZE) .E. HTAB                                             *042 LM00055    
                            SIZE = SIZE - 1                                                       *043 LM00056    
                            TRANSFER TO STEST                                                     *044 LM00057    
                          END OF CONDITIONAL                                                      *045 LM00058    
                        END OF CONDITIONAL                                                        *046 LM00059    
                                                                                                       LM00060    
                      OR WHENEVER LINE(I) .E. KILL                                                *047 LM00061    
                1           .AND. LINE(SIZE) .NE. BACKSP                                          *047 LM00062    
                2           .AND. LINE(I+1) .NE. BACKSP                                           *047 LM00063    
                        SIZE = 0                                                                  *048 LM00064    
                                                                                                       LM00065    
                      OR WHENEVER LINE(I) .E. MCNG                                                *049 LM00066    
                        WHENEVER LINE(I+1) .E. CAPE .OR. LINE(I+1) .E. LILE                       *050 LM00067    
                1           , ERASE = LINE(I+2)                                                   *050 LM00068    
                        WHENEVER LINE(I+1) .E. CAPK .OR. LINE(I+1) .E. LILK                       *051 LM00069    
                1           , KILL = LINE(I+2)                                                    *051 LM00070    
                        I = I + 2                                                                 *052 LM00071    
                                                                                                       LM00072    
                      OTHERWISE                                                                   *053 LM00073    
                        SIZE = SIZE + 1                                                           *054 LM00074    
                        LINE(SIZE) = LINE(I)                                                      *055 LM00075    
                      END OF CONDITIONAL                                                          *056 LM00076    
      LLP           CONTINUE                                                                      *057 LM00077    
                    FUNCTION RETURN                                                               *058 LM00078    
                                                                                                       LM00079    
                  END OF FUNCTION                                                                 *059 LM00080    
