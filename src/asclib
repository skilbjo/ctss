                  ENTRY TO TYPOUT.                                                                *033 LM00054    
                    INESC.(LINE,BUFSIZ,XLINE,SIZE,361)                                            *034 LM00055    
                    SETFUL.                                                                       *035 LM00056    
                    K = 0                                                                         *036 LM00057    
                    THROUGH PRLOOP, FOR J = 1, 1, J .G. SIZE                                      *037 LM00058    
                      TWORD = (TWORD .LS. NBITS) .V. ASCTSS.(XLINE(J))                            *038 LM00059    
                      K = K + 1                                                                   *039 LM00060    
                      WHENEVER K .GE. WRDSIZ/NBITS                                                *040 LM00061    
                        K = 0                                                                     *041 LM00062    
                        PRWORD.(TWORD)                                                            *042 LM00063    
