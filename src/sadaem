                  DIMENSION ENTRY(7),USER(7)                                                      *003 LM00003    
                  DIMENSION BUFF1(432),BUFF2(432)                                                 *004 LM00004    
                  EQUIVALENCE (N1,ENTRY(7)),(N2,ENTRY(6)),(PB,USER(7)),(PG,USER(6))               *005 LM00005    
