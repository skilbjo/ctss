                  ATTACH.(MFD,FILE,WOOPS)                                                         *020 LM00020    
                  OPEN.($R$,UFD,FILE,MZE,MZE,WOOPS)                                               *021 LM00021    
                  BUFFER.(UFD,FILE,BUFF1(432)...432,WOOPS)                                        *022 LM00022    
