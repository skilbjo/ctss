                         GRAPH1                                                                        LM00001    
                  EXTERNAL FUNCTION (X,Y,NN,S,LS,XMIN,DX,YMIN,DY,I)                               *001 LM00002    
                  INSERT FILE .XR.                                                                *002 LM00003    
                  DEFINE UNARY OPERATOR .XR., PRECIDENCE SAME AS -                                *003 LM00001    
                  MODE STRUCTURE .XR.1 TO 1                                                       *004 LM00002    
             JMP      *+2,MQ,*+1                                                                  *005 LM00003    
             JMP      *+2,AC,*+3                                                                  *006 LM00004    
             JMP      *+7,BT,*+9                                                                  *007 LM00005    
             JMP      *+2,BT,*+10                                                                 *008 LM00006    
             CLA      B                                                                           *009 LM00007    
             ALS      15                                                                          *010 LM00008    
             STT      LOC+1                                                                       *011 LM00009    
             PXA      0,0                                                                         *012 LM00010    
             OUT      AC                                                                          *013 LM00011    
             XCA                                                                                  *014 LM00012    
             JMP      *-5                                                                         *015 LM00013    
             STQ      T                                                                           *016 LM00014    
             JMP      *-8                                                                         *017 LM00015    
             STO      T                                                                           *018 LM00016    
             JMP      *-10                                                                        *019 LM00017    
             END                                                                                  *020 LM00018    
                  ENTRY TO GRAPH1.                                                                *021 LM00004    
                         GRAPH1 IS CALLED BY SCLGPH WITH I=1 OR BY GRAPH                               LM00005    
                         WITH I=2 AND XMIN,DX,YMIN,DY SET TO 0,1,0,                                    LM00006    
                  EQUIVALENCE (AN,NNN)                                                            *022 LM00007    
                  NNN=NN                                                                          *023 LM00008    
                  LLS=LS                                                                          *024 LM00009    
                  II=I                                                                            *025 LM00010    
                         CHECK THAT NUMBER OF ARGUMENTS =10.                                           LM00011    
                  EXECUTE NUMP.(-8,N,.XR.4)                                                       *026 LM00012    
                  WHENEVER (N-10).E.0, TRANSFER TO QQ0005                                         *027 LM00013    
                  EXECUTE ERNOTE.(2,QQ0007(1))                                                    *028 LM00014    
                  VECTOR VALUES QQ0006=777777777777K,$GRAPH1$                                     *029 LM00015    
                  EQUIVALENCE (QQ0006,QQ0007)                                                     *030 LM00016    
                  DIMENSION QQ0007(1)                                                             *031 LM00017    
                         DETERMINE WHICH END OF PLOT PEN IS NEAREST                                    LM00018    
      QQ0005      N=.ABS.(NNN)                                                                    *032 LM00019    
                  EXECUTE WHERE.(XN,YN)                                                           *033 LM00020    
                  XJ=(X(1)-XMIN)/DX                                                               *034 LM00021    
                  YJ=(Y(1)-YMIN)/DY                                                               *035 LM00022    
                  D1=SQRT.((XJ-XN).P.2+(YJ-YN).P.2)                                               *036 LM00023    
                  XJ=(X(N)-XMIN)/DX                                                               *037 LM00024    
                  YJ=(Y(N)-YMIN)/DY                                                               *038 LM00025    
                  DN=SQRT.((XJ-XN).P.2+(YJ-YN).P.2)                                               *039 LM00026    
                  WHENEVER D1.G.DN, TRANSFER TO QQ0008                                            *040 LM00027    
                         SET TO DRAW FROM LEFT TO RIGHT                                                LM00028    
                  J=1                                                                             *041 LM00029    
                  K=1                                                                             *042 LM00030    
                  TRANSFER TO QQ0010                                                              *043 LM00031    
                         SET TO DRAW FROM RIGHT TO LEFT                                                LM00032    
      QQ0008      J=N                                                                             *044 LM00033    
                  K=-1                                                                            *045 LM00034    
                         PEN UP BETWEEN POINTS TEST                                                    LM00035    
      QQ0010      WHENEVER NNN.GE.0, TRANSFER TO QQ0011                                           *046 LM00036    
                         SET FOR PEN UP BETWEEN POINTS                                                 LM00037    
                  I3=3                                                                            *047 LM00038    
                  N1=-1                                                                           *048 LM00039    
                  TRANSFER TO QQ0013                                                              *049 LM00040    
                         SET FOR PEN DOWN BETWEEN POINTS                                               LM00041    
      QQ0011      I3=2                                                                            *050 LM00042    
                  N1=-2                                                                           *051 LM00043    
      QQ0013      XJ=(X(J)-XMIN)/DX                                                               *052 LM00044    
                  YJ=(Y(J)-YMIN)/DY                                                               *053 LM00045    
                         MOVE TO 1ST POINT WITH PEN UP                                                 LM00046    
                  EXECUTE PLOT1.(XJ,YJ,3)                                                         *054 LM00047    
                  THROUGH QQ0014, FOR III=1,1,III.G.N                                             *055 LM00048    
                         TEST IF COORDINATES MUST BE SCALED                                            LM00049    
                  TRANSFER TO QQ0016(II)                                                          *056 LM00050    
      QQ0016(01)  XJ=(X(J)-XMIN)/DX                                                               *057 LM00051    
                  YJ=(Y(J)-YMIN)/DY                                                               *058 LM00052    
                  TRANSFER TO QQ0018                                                              *059 LM00053    
      QQ0016(02)  XJ=X(J)                                                                         *060 LM00054    
                  YJ=Y(J)                                                                         *061 LM00055    
                         TEST IF SYMBOL PLOT                                                           LM00056    
      QQ0018      WHENEVER (S).E.0, TRANSFER TO QQ0020                                            *062 LM00057    
                         IF SYMBOL PLOT DRAW SYMBOL                                                    LM00058    
                  EXECUTE SYMBL5.(XJ,YJ,S,LLS,0,N1)                                               *063 LM00059    
                  TRANSFER TO QQ0014                                                              *064 LM00060    
                         MOVE TO NEXT POINT                                                            LM00061    
      QQ0020      EXECUTE PLOT1.(XJ,YJ,I3)                                                        *065 LM00062    
                  WHENEVER NN.GE.0, TRANSFER TO QQ0014                                            *066 LM00063    
                         IF POINT PLOT DROP PEN AT POINT                                               LM00064    
                  EXECUTE PLOT1.(XJ,YJ,2)                                                         *067 LM00065    
      QQ0014      J=J+K                                                                           *068 LM00066    
                  M=J-K                                                                           *069 LM00067    
                  XJ=(X(M)-XMIN)/DX                                                               *070 LM00068    
                  YJ=(Y(M)-YMIN)/DY                                                               *071 LM00069    
                         LIFT PEN AT LAST POINT                                                        LM00070    
                  EXECUTE PLOT1.(XJ,YJ,3)                                                         *072 LM00071    
                  FUNCTION RETURN                                                                 *073 LM00072    
                  INTEGER NNN,NN,LLS,LS,II,I                                                      *074 LM00073    
                  INTEGER NUMP.,N,J,K,I3,N1                                                       *075 LM00074    
                  INTEGER III,M                                                                   *076 LM00075    
                  END OF FUNCTION                                                                 *077 LM00076    
